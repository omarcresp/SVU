  "CFD.Hector.SocialPage.cs.11636": "(spouse)",
  "CFD.Hector.SocialPage.cs.11637": "(spouse)",
  "CFD.Hector.SocialPage.cs.11639": "(partner)",
  "CFD.Hector.SocialPage.cs.11640": "(partner)",
